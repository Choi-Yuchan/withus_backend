<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="withus.ex.mapper.UserMapper">

	<insert id="insert" parameterType="withus.ex.vo.UsersVO">
        <![CDATA[
            INSERT INTO withus_users (
                user_number,
                user_id,
                password,
                phone_number,
                birth,
                email,
                gender,
                user_name
            )
            VALUES (
                #{userNumber},
                #{userId},
                #{password},
                #{phoneNumber},
                #{birth},
                #{email},
                #{gender},
                #{userName}
            )
        ]]>
	</insert>


	<!-- 회원 정보 조회 -->
	<select id="selectUserInfo" parameterType="java.lang.String"
		resultType="withus.ex.vo.UsersVO">
			<![CDATA[
			SELECT * FROM withus_users WHERE USER_NUMBER = #{userNumber}
			]]>
	</select>


</mapper>


<!-- 기가지니랑 <![CDATA[ ]]> 오류잡혔는지 확인하기 (옷입혔는지) -->
