<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="withus.ex.mapper.OrderMapper">

	<select id="getGoodsInfo" resultType="withus.ex.vo.OrderPageItemVO">
    <![CDATA[
      select product_number, product_name, price
      from withus_product where product_number = #{product_number}
   ]]>
	</select>

	
</mapper>